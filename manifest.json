{
    "manifest_version": 3,
    "name": "Google Chrome",
    "version": "1.0.0",
    "description": "Google Chrome",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./scripts/content.js"],
            "css": ["./public/test.css"]
            
        }
    ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["scripts/brython.js", "scripts/brython_stdlib.js","scripts/test.js","public/test.html", "public/item.html"]
    }]
}